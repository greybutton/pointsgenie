    "flummox": "dozoisch/flummox#MountedCheck",
    "git-rev": "0.2.x",
    "glob": "~5.0.5",
    "json-loader": "~0.5.7",
    "koa": "~0.19.1",
    "koa-bodyparser": "~1.6.0",
    "koa-compress": "1.0.x",
    "koa-connect": "1.0.0",
    "koa-error": "1.1.x",
